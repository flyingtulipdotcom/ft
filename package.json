    "@layerzerolabs/lz-v2-utilities": "^3.0.130",
    "@layerzerolabs/metadata-tools": "^3.0.2",
    "@layerzerolabs/oapp-evm": "^0.3.2",
    "@layerzerolabs/oft-evm": "^3.2.1",
    "@layerzerolabs/prettier-config-next": "^3.0.130",
    "@layerzerolabs/solhint-config": "^3.0.130",
    "@layerzerolabs/test-devtools-evm-hardhat": "^0.5.2",
    "@layerzerolabs/toolbox-hardhat": "^0.6.12",
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
